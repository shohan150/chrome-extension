{
   "manifest_version": 3,
   "name": "Coding train extension",
   "version": "0.0.1",
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "content.js"
         ]
      }
   ],
   "background": {
      "service_worker": "background.js"
   },
   "action": {
      "default_icon": "icon.png"
   },
   "permissions": [
      "activeTab"
   ]
}