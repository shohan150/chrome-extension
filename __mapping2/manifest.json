{
   "manifest_version": 3,
   "name": "Data Mapping Extension",
   "version": "0.0.1",
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "content.js"
         ]
      }
   ],
   "action": {
      "default_icon": "img/icon.png",
      "default_popup": "popup.html",
      "persistent": true
   },
   "permissions": [
      "activeTab",
      "tabs",
      "storage"
   ],
   "options_page": "options.html"
}