{
   "name": "Add content using background script",
   "version": "0.0.1",
   "description": "Add additional content to the page",
   "manifest_version": 3,
   "background": {
      "service_worker": "background.js"
   },
   "permissions": [
      "scripting"
   ],
   "host_permissions": [
      "<all_urls>"
   ]
}