{
   "name": "Add content",
   "version": "0.0.1",
   "description": "Add additional content to the page using content script",
   "manifest_version": 3,
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "content.js"
         ],
         "css": [
            "content.css"
         ]
      }
   ],
   "host_permissions": [
      "<all_urls>"
   ]
}